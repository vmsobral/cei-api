{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "gelf",
        "options": {
          "tag": "ecs-APPNAME",
          "gelf-address": "udp://localhost:12201"
        }
      },
      "entryPoint": [],
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "command": [
        "/opt/app/application-1.0/bin/application"
      ],
      "environment": [
        {
          "name": "JAVA_OPTS",
          "value": "-javaagent:/opt/datadog/dd-java-agent-0.31.0.jar -Xms128m -Xmx512m"
        },
        {
          "name": "DD_SERVICE_NAME",
          "value": "quickstart"
        },
        {
          "name": "DD_AGENT_HOST",
          "value": "datadog-agent.guiabolso.in"
        },
        {
          "name": "DBNAME_DATASOURCE_URL",
          "value": "database_url"
        },
        {
          "name": "DBNAME_DATASOURCE_USERNAME",
          "value": "dbName"
        },
        {
          "name": "DBNAME_DATASOURCE_PASSWORD",
          "value": "dbPass"
        },
        {
          "value": "DBNAME_DATASOURCE_PASSWORD_ENCRYPTED",
          "name": "true"
        },
        {
          "value": "DBNAME_DATASOURCE_DDL",
          "name": "update"
        },
        {
          "name": "DATA_ENCRYPTION_THREAD_COUNT",
          "value": "20"
        },
        {
          "name": "DATA_ENCRYPTION_KEY",
          "value": "kmsAppKey"
        },
        {
          "name": "S3_BUCKET_NAME",
          "value": "gb-quickstart"
        },
        {
          "name": "S3_ENDPOINT",
          "value": "s3.amazonaws.com"
        },
        {
          "name": "S3_REGION",
          "value": "sa-east-1"
        }
      ],
      "memory": 1024,
      "cpu": 200,
      "image": "repodocker/APPNAME:COMMIT",
      "privileged": true,
      "name": "APPNAME",
      "essential": true
    }
  ],
  "taskRoleArn": "arn:aws:iam::744617668409:role/ROLE",
  "family": "APPNAME",
  "networkMode": "bridge",
  "volumes": [],
  "placementConstraints": []
}
